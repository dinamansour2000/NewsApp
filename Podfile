# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'NewsApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NewsApp

 pod 'Alamofire'
    pod 'ObjectMapper'
    pod 'SwiftyJSON'
    pod 'AlamofireObjectMapper'


    
    # Layout Constraints
    pod 'Cartography'
    
    #Localization
    pod 'Localize-Swift'
    
    #Image Downloader
    pod 'Kingfisher'
    
    
    #Database caching
    pod 'RealmSwift'
    pod "ObjectMapper+Realm"
    
   
    
    #Messages
    pod 'SwiftMessages'
    
    #network
    pod 'ReachabilitySwift', '~> 4.1'
    


  target 'NewsAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  

end
